    "\n",
    "secret = (random.randint(1,10))\n",
    "\n",
    "guess = input('Guess a number between 1 and 10: ')\n",
    "\n",
    "guess = int(guess)\n",
    "\n",
    "if guess > secret:\n",
    "    print('too high')\n",
    "\n",
    "elif guess < secret:\n",
    "    print('too low')\n",
    "\n",
    "else:\n",
    "    print('just right')"
   ]
